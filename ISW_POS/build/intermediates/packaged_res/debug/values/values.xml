<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="libraryColorAccent">#84A0DD</color>
    <color name="libraryColorGray">#707070</color>
    <color name="libraryColorPrimary">#320965</color>
    <color name="libraryColorRed">#FF0000</color>
    <color name="libraryColorWhite">#FFFFFF</color>
    <color name="libraryColorWhite70">#B3FFFFFF</color>
    <color name="libraryGradientEnd">#446ABE</color>
    <color name="libraryGradientMid">#96B0E6</color>
    <color name="libraryGradientStart">#9DB6EA</color>
    <color name="libraryTranCompleteColor">#24E979</color>
    <style name="AltButton" parent="android:style/Widget.Material.Button">
        <item name="android:textColor">@color/libraryColorWhite</item>
        <item name="android:background">@drawable/library_alt_background</item>
        <item name="background">@drawable/library_alt_background</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">16sp</item>
        <item name="android:layout_height">48sp</item>
        <item name="android:layout_margin">20sp</item>
    </style>
    <style name="Button" parent="android:style/Widget.Material.Button">
        <item name="android:textColor">@color/libraryColorPrimary</item>
        <item name="android:background">@drawable/library_button_background</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">16sp</item>
        <item name="android:layout_height">48sp</item>
        <item name="android:layout_margin">20sp</item>
    </style>
    <style name="CircularProgressBar">
        <item name="android:indeterminateOnly">false</item>
        <item name="android:progressDrawable">@drawable/library_circle</item>
    </style>
    <style name="Library_Theme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:colorPrimary">@color/libraryColorPrimary</item>
        <item name="colorPrimary">@color/libraryColorPrimary</item>
        <item name="android:colorAccent">@color/libraryColorAccent</item>
        <item name="colorAccent">@color/libraryColorAccent</item>
        <item name="android:fontFamily">@font/nunito</item>
        <item name="fontFamily">@font/nunito</item>
        <item name="android:textViewStyle">@style/TextStyle</item>
        <item name="buttonStyle">@style/Button</item>
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>
    <style name="TextStyle" parent="android:style/TextAppearance.Material">
        <item name="fontFamily">@font/nunito</item>
        <item name="android:fontFamily">@font/nunito</item>
    </style>
</resources>